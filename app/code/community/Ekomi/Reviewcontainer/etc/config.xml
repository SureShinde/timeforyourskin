<config>
    <modules>
        <Ekomi_Reviewcontainer>
            <version>0.1.2</version>
        </Ekomi_Reviewcontainer>
    </modules>
    <global>
        <helpers>
            <reviewcontainer>
                <class>Ekomi_Reviewcontainer_Helper</class>
            </reviewcontainer>
        </helpers>
        <blocks>
            <reviewcontainer>
                <class>Ekomi_Reviewcontainer_Block</class>
            </reviewcontainer>
        </blocks>
        <models>
            <reviewcontainer>
                <class>Ekomi_Reviewcontainer_Model</class>
            </reviewcontainer>
        </models>
    </global>
    <frontend>
        <layout>
            <updates>
                <reviewcontainer>
                    <file>layout.xml</file>
                </reviewcontainer>
            </updates>
        </layout>
    </frontend>
</config>
